{
    "name": "wx-service-config",
    "version": "0.0.1",
    "description": "Serve config files for projects",
    "repository": "github:RaffaeleCanale/wx-service-config",
    "license": "MIT",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "mkdir -p dist && babel src -s -D -d dist",
        "watch": "npm run build -- -w",
        "dev": "nodemon -w src --exec \"npm run build && npm run start\"",
        "debug": "node --inspect-brk dist/index.js"
    },
    "author": "Raffaele Canale",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-plugin-resolver": "^1.1.0",
        "babel-preset-es2015": "^6.24.1",
        "eslint": "^4.18.1",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.9.0"
    },
    "dependencies": {
        "bcrypt": "^2.0.0",
        "body-parser": "^1.18.2",
        "compression": "^1.7.2",
        "cors": "^2.8.4",
        "express": "^4.16.3",
        "express-fileupload": "^0.4.0",
        "express-jwt": "^5.3.1",
        "express-unless": "^0.5.0",
        "joi": "^13.1.2",
        "js-utils": "git://github.com/RaffaeleCanale/js-utils.git#4d1c68ba54e388b0aff49c1f40d194d239d566e0",
